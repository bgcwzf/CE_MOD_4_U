          <AssemblerScript>[enable]
{$lua}
if syntaxcheck then return end

--active_cheat_item_by_description_name("【AA】启用脚本：获取当前星系对象(StarData)与主GameData对象",true)

if readPointer(getAddressSafe("dsp_GameMainData"))==nil then return end

active_cheat_item_by_description_name("【LUA】机甲在太空中的普通飞行速度n倍速",true)

if player_uspeed_times_addr==nil then
  error("激活失败，需要运行前置脚本")
end


if not slow_increase_change_uspeed_timer then
   local x,y,z=get_player_uspeed()
   if x ~= nil then
      local sign
      sign=math.sqrt(x*x+y*y+z*z)
      --[[x=x/sign
      y=y/sign
      z=z/sign
      x=math.abs(x)]]
      writeFloat(player_uspeed_times_addr,sign/1000.0) -- 换算出当前的速度倍率
   end
   slow_increase_change_uspeed_timer = createTimer(nil)
   slow_increase_change_uspeed_timer.OnTimer = function(slow_increase_change_uspeed_timer)
                   --code here
                   local delta=182
                   --print("timer: speed delta=",delta)
                   writeFloat(player_uspeed_times_addr,readFloat(player_uspeed_times_addr)+delta)
                   delta=readFloat(player_uspeed_times_addr)
                   if delta&gt;14000.0 then
                     active_cheat_item_by_description_name("【LUA】机甲在太空中逐步加速的过程与效果3",false)
                     --active_cheat_item_by_description_name("【LUA】机甲在太空中逐步加速的过程与效果2",true)
                   end
                 end
   slow_increase_change_uspeed_timer.Interval = 250 --delay
   slow_increase_change_uspeed_timer.Enabled = true
else
    slow_increase_change_uspeed_timer.Enabled = true
end



[disable]
{$lua}
if (slow_increase_change_uspeed_timer) then
   slow_increase_change_uspeed_timer.Enabled = false
   slow_increase_change_uspeed_timer.destroy()
end
slow_increase_change_uspeed_timer=nil

