function get_star_id(star_obj)
    local id_addr=star_obj+0x50
    return readInteger(id_addr)
end

function get_star_id_of_sphere_obj(sphere_obj)
    local star_obj=sphere_obj+0x18
    star_obj=readPointer(star_obj)
    return get_star_id(star_obj)
  end
function get_stars_count()
    local gamedata=readPointer(getAddressSafe("dsp_GameMainData"))
    local galaxy=gamedata+0x38
    local galaxydata=readPointer(galaxy)
    galaxydata=galaxydata+0x3c
    return readInteger(galaxydata)
end
function get_spheres_array_of_game_data()
    local spheres_arr=readPointer(getAddressSafe("dsp_GameMainData"))
    spheres_arr=spheres_arr+0x78
    spheres_arr=readPointer(spheres_arr)
    spheres_arr=spheres_arr+0x20 -- C# Array Header Size
    registerSymbol("dsp_spheres_array",spheres_arr)
    return spheres_arr
end

function find_current_sphere(cur_star_obj)
    if cur_star_obj == nil then return end
    local cur_star_id=get_star_id(cur_star_obj)
    local spheres_arr=get_spheres_array_of_game_data()
    local all_stars_count=get_stars_count()
    local i=0
    local sid=0
    local item=nil
    while i&lt;all_stars_count do
      item=readPointer(spheres_arr+i*8)
      if item~=nil and item ~=0 then
        sid=get_star_id_of_sphere_obj(item)
        if sid==cur_star_id then
            registerSymbol("current_sphere",item)
            local layer_sorted_table=readPointer(item+0x28)+0x20
            registerSymbol("current_layer_table",layer_sorted_table)

            local layer_table_id_based=readPointer(item+0x30)+0x20
            registerSymbol("current_layer_table_id_based",layer_table_id_based)
          return item
        end
      end
      i=i+1
    end
    return nil
  end
function refresh_global_objs()
  getGameMainObj()
  getGameCameraObj()
  getUnityCameraObj()
  getLocalStarObj()
  getLocalPlanetObj()
  getMainPlayerObj()
  getGalaxyObj()
  find_current_sphere(getAddressSafe("dsp_LocalStarData"))
end
