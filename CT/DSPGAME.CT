  writeDouble(player_uspeed_times_addr+8*2,1000.0)
  writeDouble(player_uspeed_times_addr+8*3,1000.0)
  ]]
end
if last_player_uspeed_times_value_str==nil then
last_player_uspeed_times_value_str=''
end
function set_uspeed_times()
  local times=readFloat(player_uspeed_times_addr)
  if times==nil then return end
  local s=string.format("%f",times)
  if last_player_uspeed_times_value_str==s then return end
  last_player_uspeed_times_value_str=s

  local game_data = getAddressSafe("dsp_MainPlayer")
  if game_data == nill then return end
  local player=game_data --readPointer(game_data)
  if player == 0 then return end
  local u_speed=player+0x138 -- this the speed of x

  local x,y,z
  x=readDouble(u_speed)
  y=readDouble(u_speed+8)
  z=readDouble(u_speed+8*2)

  --[[ 因为目的是让xyz三个分速度的合速度增大指定的倍数，并保持速度的方向不变
  直接将原始速度分量乘以times（即new_x = times * x）会改变速度向量的方向，
  因为这样仅仅是将每个分量按相同的比例增加，而不是保持它们之间的相对比例关系。
  当原始速度向量不是沿着一个主轴时，这将导致速度向量方向的改变。
  因此，为了保持原始速度方向不变，必须首先将速度向量标准化，然后按比例增加其大小。
  ]]--
  local real_global_speed=math.sqrt(x*x+y*y+z*z)
  -- 计算速度向量每个分量相对于其总长度的比例（标准化）
  x=x/real_global_speed
  y=y/real_global_speed
  z=z/real_global_speed
  --print(u_speed,":",sign,":",times,":end")
  -- 常数1000是游戏内部的速度固有系数
  writeDouble(u_speed,1000*times*x)
  --writeDouble(player_uspeed_times_addr+8,1000*times*x)

  u_speed=u_speed+8
  writeDouble(u_speed,1000*times*y)
  --writeDouble(player_uspeed_times_addr+8*2,1000*times*y)

  u_speed=u_speed+8
  writeDouble(u_speed,1000*times*z)
  --writeDouble(player_uspeed_times_addr+8*3,1000*times*z)

  --[[
  -- 之前这种在原有速度上直接加倍的方案太夸张了
  writeDouble(u_speed,readDouble(u_speed)*times)
  u_speed=u_speed+8
  writeDouble(u_speed,readDouble(u_speed)*times)
  u_speed=u_speed+8
  writeDouble(u_speed,readDouble(u_speed)*times)
  ]]
end

function get_player_uspeed() -- 此函数返回的是一个3元的 turple: x,y,z 出错时返回 nil,nil,nil
  local game_data = getAddressSafe("dsp_MainPlayer")
  if game_data == nil or game_data==0 then
   print("nil dsp_game_main_player")
   return nil,nil,nil
  end
  if game_data == nill then return end
  local player= game_data -- readPointer(game_data)
  if player == nil or player==0 then
   print("nil player")
   return nil,nil,nil
  end
  local u_speed=player+0x138 -- this the speed of x

  --print("player:",player,"addr u_speed:",u_speed)
  local x,y,z
  x=readDouble(u_speed)
  y=readDouble(u_speed+8)
  z=readDouble(u_speed+8*2)
  return x,y,z
end

if not change_uspeed_timer then
   change_uspeed_timer = createTimer(nil)
   change_uspeed_timer.OnTimer = function(change_uspeed_timer)
                   --code here
                   set_uspeed_times()
                 end
   change_uspeed_timer.Interval = 250 --delay
